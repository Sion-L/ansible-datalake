MIN_UNIX_USER_ID_TO_SYNC = 500
MIN_UNIX_GROUP_ID_TO_SYNC = 500
SYNC_INTERVAL = 1
unix_user=hadoop
unix_group=hadoop

rangerUsersync_password={{ ranger_admin_password }}
usersync_principal=
usersync_keytab=
hadoop_conf={{ hadoop_conf_dir }}
CRED_KEYSTORE_FILENAME=/etc/ranger/usersync/conf/rangerusersync.jceks
